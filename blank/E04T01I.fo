										<fo:leader leader-pattern="space" leader-length="5pt"/>
											<xsl:call-template name="body_id">
												<xsl:with-param name="id" select="'LIKV_EDRPOU'"/>
											</xsl:call-template>
										</fo:block>
									</fo:table-cell>
								</fo:table-row>
							</fo:table-body>
						</fo:table>
						<fo:table>
							<fo:table-column column-number="1" column-width="5cm"/>
							<fo:table-column column-number="2"/>
							<fo:table-body>
